{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [2, "tab"],
        "no-tabs": 0,
        "linebreak-style": ["error", "unix"],
        "quotes": ["error", "single"],
        "semi": ["error", "never"],
        "properties": "never" 
    }
}
